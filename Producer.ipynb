   "source": [
    "from kafka import KafkaProducer as kp\n",
    "import random\n",
    "producer = kp(bootstrap_servers=\"127.0.0.1:9092\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "5b70ef28",
   "metadata": {},
   "outputs": [],
   "source": [
    "for x in range(10):\n",
    "    n = random.random()\n",
    "    producer.send(\"mensagens\",key=b\"Chave %d \" % x, value=b\"Mensagem %f \" % n)"
   ]
  },
